<div class="window-layerlist"{{if !setOptions.isShowLayer}}style="display:none;"{{/if}}>
    <div class="layerlist-box win-box">
        <div class="win-header win-layerlist-header" data-hdy-ev="mousedown:layer_dragstart">
            <ul class="win-header-tabs-1 layerlist-tabs">
                <li class="current">图层</li>
            </ul>
            <div class="win-header-close win-layerlist-close" data-hdy-ev="click:closeWin"></div>
        </div>
        <div class="win-body layerlist-body">
            <div class="layerlist-topbar">
                <div class="layerlist-del-btn" data-hdy-ev="click:deleteElement"></div>
            </div>
            <ul class="layerlist">
            </ul>
        </div>
    </div>
</div>